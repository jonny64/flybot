<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="NSIS Script"
	Version="8,00"
	>
	<Rules>
		<CustomBuildRule
			Name="NSIS Script"
			DisplayName="nsis script"
			CommandLine="[NsisMake] [inputs]"
			Outputs="$(InputName).exe"
			AdditionalDependencies="$(SolutionDir)bin\Chatbot.dll"
			FileExtensions="*.nsi"
			>
			<Properties>
				<StringProperty
					Name="NsisMake"
					DisplayName="NSIS make"
					Switch="[value]"
					DefaultValue="&quot;C:\Program Files\NSIS\makensis&quot;"
				/>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
